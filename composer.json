{
    "name": "kabiroman/adaptive-entity-manager-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for kabiroman/adaptive-entity-manager integration",
    "keywords": ["symfony", "bundle", "entity", "manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ruslan Kabirov",
            "email": "kabirovruslan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "kabiroman/adaptive-entity-manager": "^1.0",
        "doctrine/orm": "^3.3",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Kabiroman\\AdaptiveEntityManagerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kabiroman\\AdaptiveEntityManagerBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "symfony/phpunit-bridge": "^6.4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
